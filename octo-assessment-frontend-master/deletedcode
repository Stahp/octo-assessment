INSERT_SPACES_REQUEST,
INSERT_SPACES_SUCCESS,
INSERT_SPACES_FAILURE

const insertSpacesRequest = () => {
    return {
        type: INSERT_SPACES_REQUEST
    }
}

const insertSpacesSuccess = spaces => {
    return {
        type: INSERT_SPACES_SUCCESS,
        payload: spaces
    }
}

const insertSpacesFailure = error => {
    return {
        type: INSERT_SPACES_FAILURE,
        payload: error
    }
}

<Login {...props} type= "SpaceDetail"/>


